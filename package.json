{
  "name": "philips-phlowchart",
  "version": "0.1.1",
  "description": "A Grafana panel plugin to render interactive flowchart visualization of directed graph data.",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch"
  },
  "author": "Sanjay Dandekar",
  "license": "Apache-2.0",
  "resolutions": {
    "minimist": "1.2.6",
    "glob-parent": "5.1.2",
    "d3-selection": "3.0.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "moment": "2.29.4"
  },
  "devDependencies": {
    "@grafana/data": "9.0.7",
    "@grafana/toolkit": "9.0.7",
    "@grafana/ui": "9.0.7",
    "@types/dagre": "0.7.47",
    "emotion": "10.0.27"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "dagre": "0.8.5",
    "react-flow-renderer": "10.0.7"
  }
